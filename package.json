{
  "name": "react-honeycomb-loading-spinner-sass",
  "version": "1.0.8",
  "description": "Loading animation based on work by Kevin Martin and Doug Harper",
  "main": "build/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src --out-dir build",
    "watch": "babel src --out-dir build --watch",
    "test": "NODE_ENV=test mocha --require test/setup.js --compilers js:babel-register test/**/*_spec.jsx",
    "test-watch": "NODE_ENV=test mocha --require test/setup.js --compilers js:babel-register test/**/*_spec.jsx --growl --watch --watch-extensions jsx"
  },
  "author": "Edwin Knuth <eknuth@gmail.com> (http://iknuth.com.com)",
  "license": "MIT",
  "repository": "git@github.com:toshok/react-honeycomb-loading-spinner.git",
  "dependencies": {
    "react": "^0.14.3"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "growl": "^1.8.1",
    "jsdom": "^7.2.0",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3"
  }
}
